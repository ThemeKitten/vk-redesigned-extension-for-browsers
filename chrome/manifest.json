{
   "content_scripts": [ {
      "css": [ "pages.css", "main.css", "images.css" ],
      "matches": [ "*://vk.com/*" ],
      "run_at": "document_start"
   } ],
   "web_accessible_resources": [
        "main.css", "pages.css", "images.css"
    ],
   "description": "Extension which improves design of VK.com.",
   "homepage_url": "http://vk.com/vkredesigned",
   "icons": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
   },
   "name": "VK Redesigned",
   "short_name": "VK Redesign",
   "permissions": [ "*://vk.com/*", "storage" ],
   "version": "1.4",
   "manifest_version": 2,
   "key": "",
   "update_url": "https://clients2.google.com/service/update2/crx"
}